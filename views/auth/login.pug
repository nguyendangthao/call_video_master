// section
#contact.contact-box
  .container
    .row
      .col-lg-7.col-sm-7.col-xs-12
        .contact-block
          if errors
            .alert.alert-danger= errors
          form(action='/auth/login', method='post')
            .form-group.group-input
              input#email(type='email', name='email',class="form-control", placeholder='mail@example.com', title='Please enter a valid email', required='') 
            .form-group.group-input
              input#firstname(type='password', name='password',class="form-control", placeholder='Password', title='Please enter your password', required='')
            input.login(type='submit', value='Sign In', class='btn btn-primary')
      .col-lg-5.col-sm-5.col-xs-12
        .left-contact
          .media.cont-line
            .media-left.icon-b
              i.fa.fa-location-arrow(aria-hidden='true')
            .media-body.dit-right
              h4 Address
              p Fleming 196 Woodside Circle Mobile, FL 36602
          .media.cont-line
            .media-left.icon-b
              i.fa.fa-envelope(aria-hidden='true')
            .media-body.dit-right
              h4 Email
              a(href='#') demoinfo@gmail.com
              br
              a(href='#') demoinfo@gmail.com
          .media.cont-line
            .media-left.icon-b
              i.fa.fa-volume-control-phone(aria-hidden='true')
            .media-body.dit-right
              h4 Phone Number
              a(href='#') 12345 67890
              br
              a(href='#') 12345 67890
